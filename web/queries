insert into USERS values('jahziah@kth.se', 'Jahziah Wagner', md5(123))



CREATE TABLE `rebate_sharer`.`contributions` (
  `pname` VARCHAR(50)  NOT NULL,
  `orgprice` INTEGER UNSIGNED NOT NULL,
  `discprice` INTEGER UNSIGNED NOT NULL,
  `location` VARCHAR(30)  NOT NULL,
  `email` VARCHAR(30)  NOT NULL,
  `timestamp` TIMESTAMP  NOT NULL,
  PRIMARY KEY (`pname`),
  CONSTRAINT `email_fk` FOREIGN KEY `email_fk` (`email`)
    REFERENCES `USERS` (`email`)
    ON DELETE RESTRICT
    ON UPDATE RESTRICT
)
ENGINE = InnoDB;
